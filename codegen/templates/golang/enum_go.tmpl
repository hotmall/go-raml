{{define "enum_go"}}
// DO NOT EDIT THIS FILE. This file will be overwritten when re-running go-raml.

package {{.Pkg}}
type {{.Name}} {{.Type}}

const (
{{- range $k, $v := .Fields}}
    {{$v.Name}} {{.Type}} = {{$v.Value}}
{{- end}}
)

// Reset set {{.Name}} zero value
func (s *{{.Name}}) Reset() {
    {{- if eq .Type "string" }}
    *s = ""
    {{- else if eq .Type "int" }}
    *s = 0
    {{- end -}}
}
{{end}}
